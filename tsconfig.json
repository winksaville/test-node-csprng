{
  "compilerOptions": {
    "target": "es5",
    "lib": [ "es5" ],
    "noImplicitAny": true,
    "module": "commonjs",
    "moduleResolution": "Node",
    "sourceMap": true,
    "allowJs": false,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "outDir" : "build"
  },
  "files": [
    "test.ts"
  ]
}
